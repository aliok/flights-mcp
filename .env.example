# LLM Judge Configuration for gevals
# Copy this file to .env and fill in your actual credentials
# The .env file is gitignored and will not be committed to the repository

# Base URL for the judge API
JUDGE_BASE_URL=https://your-judge-api-url/v1

# API key for authentication
JUDGE_API_KEY=your-api-key-here

# Model name to use for judging
JUDGE_MODEL_NAME=your-model-name
